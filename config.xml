<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.example"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>GCC</name>

    <description>
        Guadalajara Country Club, el club deportivo, cultural y social más emblemático de la ciudad, cuna del Lorena Ochoa Invitational y promotor del deporte en un entorno familiar, desde 1909
    </description>

    <author href="https://kubik.mx" email="apps@kubik.mx">
        KubikInteractive
    </author>

    
    <gap:config-file platform="ios" parent="UIStatusBarHidden">
        <true/>
    </gap:config-file>

    <gap:config-file platform="ios" parent="UIViewControllerBasedStatusBarAppearance">
        <false/>
    </gap:config-file>

    <gap:plugin name="com.onesignal.plugins.onesignal" version="1.6.1" />

    <gap:plugin name="nl.genkgo.inappbrowser" version="0.5.0" />

    <preference name="orientation" value="portrait" />


    <!-- iOS 7.0+ -->
    <icon src="icons/Icon-60@3x.png" gap:platform="ios" width="180" height="180" />
    <icon src="icons/Icon-60.png" gap:platform="ios" width="60" height="60" />
    <icon src="icons/Icon-60@2x.png" gap:platform="ios" width="120" height="120" />
    <icon src="icons/Icon-76.png" gap:platform="ios" width="76" height="76" />
    <icon src="icons/Icon-76@2x.png" gap:platform="ios" width="152" height="152" />
    <icon src="icons/Icon-small.png" gap:platform="ios" width="29" height="29" />
    <icon src="icons/Icon-small@2x.png" gap:platform="ios" width="58" height="58" />
    <icon src="icons/Icon-40.png" gap:platform="ios" width="40" height="40" />
    <icon src="icons/Icon-40@2x.png" gap:platform="ios" width="80" height="80" />
    <!-- iOS 6.1 -->
    <icon src="icons/Icon.png" gap:platform="ios" width="57" height="57" />
    <icon src="icons/Icon@2x.png" gap:platform="ios" width="114" height="114" />
    <icon src="icons/Icon-72.png" gap:platform="ios" width="72" height="72" />
    <icon src="icons/Icon-72@2x.png" gap:platform="ios" width="144" height="144" />
    <icon src="icons/Icon-50.png" gap:platform="ios" width="50" height="50" />
    <icon src="icons/Icon-50@2x.png" gap:platform="ios" width="100" height="100" />
    <!-- Android -->
    <icon src="icons/ldpi.png" gap:platform="android" gap:qualifier="ldpi" />
    <icon src="icons/mdpi.png" gap:platform="android" gap:qualifier="mdpi" />
    <icon src="icons/hdpi.png" gap:platform="android" gap:qualifier="hdpi" />
    <icon src="icons/xhdpi.png" gap:platform="android" gap:qualifier="xhdpi" />
    <icon src="icons/xxhdpi.png" gap:platform="android" gap:qualifier="xxhdpi" />




    
</widget>
